{
  "name": "honor-fx-crm-backend",
  "version": "1.0.0",
  "description": "honor-fx-crm-backend",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "prod": "npx tsc && node dist/server.js",
    "migrate": "npx knex --knexfile ./src/knexFile.ts migrate:latest +x ts",
    "seed": "npx knex --knexfile ./src/knexFile.ts seed:run +x ts",
    "db-init": "npx knex --knexfile ./src/knexFile.ts migrate:latest +x ts && npx knex --knexfile ./src/knexFile.ts seed:run +x ts",
    "lint": "tsc --noEmit && eslint .",
    "lint-fix": "npx eslint . --fix"
  },
  "private": true,
  "dependencies": {
    "@aws-sdk/client-s3": "^3.436.0",
    "@aws-sdk/client-ses": "^3.405.0",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-useragent": "^1.0.15",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "libphonenumber-js": "^1.10.58",
    "lodash": "^4.17.21",
    "memory-cache": "^0.2.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "pg": "^8.11.3",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-useragent": "^1.0.2",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.197",
    "@types/memory-cache": "^0.2.3",
    "@types/morgan": "^1.9.5",
    "@types/morgan-json": "^1.1.0",
    "@types/node": "^20.5.7",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
